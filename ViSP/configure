ac_default_prefix=/usr/local
ac_config_libobj_dir=.
cross_compiling=no
subdirs=
MFLAGS=
MAKEFLAGS=
SHELL=${CONFIG_SHELL-/bin/sh}

# Maximum number of lines to put in a shell here document.
# This variable seems obsolete.  It should probably be removed, and
# only ac_max_sed_lines should be used.
: ${ac_max_here_lines=38}

# Identity of this package.
PACKAGE_NAME='ViSP'
PACKAGE_TARNAME='visp'
PACKAGE_VERSION='2.0.7'
PACKAGE_STRING='ViSP 2.0.7'
PACKAGE_BUGREPORT='visp@irisa.fr'

ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS INSIDE_GNOME_COMMON_TRUE INSIDE_GNOME_COMMON_FALSE CXX CXXFLAGS LDFLAGS CPPFLAGS ac_ct_CXX EXEEXT OBJEXT RANLIB ac_ct_RANLIB AR ac_ct_AR HAVE_DOT INSTALL_PROGRAM INSTALL_SCRIPT INSTALL_DATA SET_MAKE CC CFLAGS ac_ct_CC CPP X_CFLAGS X_PRE_LIBS X_LIBS X_EXTRA_LIBS QT_CXXFLAGS QT_DIR QT_LIBS QT_UIC QT_MOC HAVE_QT sim_ac_coin_configcmd HAVE_COIN OS ac_hostname CXX_VERSION SUFFIX VISP_LIBNAME VISP_HOME FG_ICCOMP_CXXFLAGS FG_ICCOMP_LDFLAGS FG_ICCOMP_LIBS BIT3_CXXFLAGS BIT3_CPPFLAGS BIT3_LDFLAGS BIT3_LIBS ROBOT_AFMA4_CXXFLAGS ROBOT_AFMA4_LDFLAGS ROBOT_AFMA4_LIBS ROBOT_AFMA6_CXXFLAGS ROBOT_AFMA6_LDFLAGS ROBOT_AFMA6_LIBS ROBOT_PTUEVI_CXXFLAGS ROBOT_PTUEVI_LDFLAGS ROBOT_PTUEVI_LIBS ROBOT_VIC2500_CXXFLAGS ROBOT_VIC2500_LDFLAGS ROBOT_VIC2500_LIBS LIBOBJS LTLIBOBJS'
ac_subst_files='build_mk debug_mk directory_mk lib_mk bin_mk clean_mk uninstall_mk depend_mk'

# Initialize some variables set by options.
ac_init_help=
ac_init_version=false
# The variables have the same names as the options, with
# dashes changed to underlines.
cache_file=/dev/null
exec_prefix=NONE
no_create=
no_recursion=
prefix=NONE
program_prefix=NONE
program_suffix=NONE
program_transform_name=s,x,x,
